{
    "title": "编程技术栈",
    "author": "零创世界",
    "description": "这是个零创世界",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "plugins": [
        "toggle-chapters",
        "mathjax",
        "-lunr",
        "-search",
        "search-pro",
        "hide-element",
        "back-to-top-button",
        "code",
        "splitter",
        "insert-logo",
        "pageview-count",
        "-sharing", 
        "sharing-plus",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "insert-logo": {
            "url": "https://upload.jianshu.io/users/upload_avatars/14645888/84d088ac-f881-45e0-8344-2388a65fd65a.png?imageMogr2/auto-orient/strip|imageView2/1/w/240/h/240",
            "style": "background: none; max-height: 100px; min-height: 50px"
        },
        "sharing": {
            "douban": true,
            "facebook": true,
            "google": true,
            "pocket": true,
            "qq": true,
            "qzone": true,
            "twitter": true,
            "weibo": true,
            "all": [
                "douban",
                "facebook",
                "google",
                "instapaper",
                "linkedin",
                "twitter",
                "weibo",
                "messenger",
                "qq",
                "qzone",
                "viber",
                "whatsapp"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "showGoTop": false
        }
    }
}
